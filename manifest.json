{
  "manifest_version": 2,
  "name": "Notes",
  "description": "notes extension to keep track of notes with any page open",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
